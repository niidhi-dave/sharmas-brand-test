.slick-slider{
  margin-bottom: 0px !important;
}
.slick-next, .slick-prev{
  z-index: 5;
}
.slick-next{
  right: 15px;
}
.slick-prev{
  left: 15px;
}
.slick-next:before, .slick-prev:before{
  color: #000;
  font-size: 26px;
}

.tagline-slider .slick-next{
  right: 70px;
}
.tagline-slider .slick-next::before{
  content: '';
  background-image: url('../assets/images/right-arrow.svg');
  background-repeat: no-repeat;
  width: 30px;
  opacity: 1;
  color: transparent;
  height: 30px;
  display: inline-block;
}
.tagline-slider .slick-prev{
  left: 70px;
}
.tagline-slider .slick-prev::before{
  content: '';
  background-image: url('../assets/images/left-arrow.svg');
  background-repeat: no-repeat;
  width: 30px;
  opacity: 1;
  color: transparent;
  height: 30px;
  display: inline-block;
}
